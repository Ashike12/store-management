{
  "name": "store-management",
  "homepage": "https://github.com/Ashike12/store-management",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "npm run dotenv:dev --  vite",
    "start": "npm run dotenv:dev --  vite",
    "build": "npm run dotenv:dev -- vite build",
    "build:prod": "npm run dotenv:prod -- vite build",
    "build:stg": "npm run dotenv:stg -- vite build",
    "dotenv:dev": "dotenv -e .env",
    "dotenv:stg": "dotenv -e .env.stg",
    "dotenv:prod": "dotenv -e .env.prod",
    "lint": "eslint .",
    "preview": "vite preview",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "format": "prettier --write ./src/**/*.{js,jsx,ts,tsx,json} --config ./.prettierrc.json",
    "postinstall": "husky install",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "lint-staged": {
    "*{.ts,.tsx,.js,.json}": "prettier --config ./.prettierrc.json --write \"./src/**/*.{ts,tsx,js,json}\""
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.3",
    "@mui/material": "^6.4.8",
    "@reduxjs/toolkit": "^2.5.1",
    "@tabler/icons-react": "^3.29.0",
    "@tailwindcss/vite": "^4.0.0",
    "class-variance-authority": "^0.7.1",
    "formik": "^2.4.6",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "react": "^19.0.0",
    "react-countup": "^6.5.3",
    "react-dom": "^19.0.0",
    "react-i18next": "^15.4.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.1.5",
    "recharts": "^2.15.2",
    "redux-persist": "^6.0.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^4.0.0",
    "vite-tsconfig-paths": "^5.1.4",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@eslint/js": "^9.19.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/lodash": "^4.17.16",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.0.4",
    "clsx": "^2.1.1",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "gh-pages": "^6.3.0",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "lint-staged": "^15.4.3",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "typescript": "~5.7.3",
    "typescript-eslint": "^8.22.0",
    "vite": "^6.0.11",
    "vitest": "^3.0.4"
  }
}
